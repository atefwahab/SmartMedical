<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.enghany.androidproject">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">


        <activity
            android:name="wmad.iti.medicalassisstantfinal.loadingActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="wmad.iti.medicalassisstantfinal.LoginActivity"
            android:label="@string/title_activity_login" />
        <activity
            android:name="wmad.iti.medicalassisstantfinal.RegisterActivity"
            android:label="@string/title_activity_register" >
        </activity>

        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">

        </activity>
        <activity
            android:name=".MedInfo"
            android:label="@string/title_activity_medinfo"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".EditActivity"
            android:label="@string/title_activity_edit"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".AddMedicine"
            android:label="@string/title_activity_add_medicine"
            android:theme="@style/AppTheme.NoActionBar"></activity>
        <!-- ATTENTION: This was auto-generated to add Google Play services to your project for
             App Indexing.  See https://g.co/AppIndexing/AndroidStudio for more information. -->
        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />

        <service
            android:name="com.example.dono.setalarm.MyAlarmService"
            android:enabled="true" />

        <receiver android:name="com.example.dono.setalarm.AlarmReceiver" />

        <activity
            android:name="com.example.dono.listView.Main2Activity"
            android:label="@string/title_activity_main2"
            android:theme="@style/AppTheme.NoActionBar" />

        <activity
            android:name="com.example.dono.setalarm.RemindActivity"
            android:label="@string/title_activity_remind"
            android:theme="@android:style/Theme.Holo.Light.Dialog" />

        <activity
            android:name="com.example.dono.listView.CustomList"
            android:label="@string/title_activity_custom_list"
            android:theme="@android:style/Theme.Holo.Light.Dialog"></activity>
    </application>

</manifest>
